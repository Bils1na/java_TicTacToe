      <Label alignment="CENTER" layoutX="94.0" layoutY="14.0" prefHeight="53.0" prefWidth="205.0" text="TicTacToe">
         <font>
            <Font name="Arial" size="28.0" />
         </font>
      </Label>
      <VBox alignment="TOP_CENTER" layoutX="131.0" layoutY="89.0" prefHeight="216.0" prefWidth="131.0">
         <children>
            <Button fx:id="playButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btnPlay" prefHeight="40.0" prefWidth="133.0" text="Play" textAlignment="CENTER">
